{
    "name": "nnnnathann/php-codegen",
    "description": "Generate client and server code based on PHP interfaces and structs",
    "type": "library",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "CodeGen\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\" : "tests/"
        }
    },
